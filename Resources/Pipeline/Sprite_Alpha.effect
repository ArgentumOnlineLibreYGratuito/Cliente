[Properties.Blend]

	ColorSrcFactor  = "SrcAlpha"
	ColorDstFactor  = "OneMinusSrcAlpha"

[Properties.Depth]

	Mask            = 0
	Condition       = "LessEqual"

[Properties.Layout]

	Attributes      = [
		[0, 0,  "Float32x3"  ],
		[1, 12, "UIntNorm8x4"],
		[2, 16, "Float32x2"  ]
	]

[Program.Vertex]

	Entry    = "vertex"
	Filename = "Resources://Pipeline/Sprite.shader"

[Program.Fragment]

	Entry    = "fragment"
	Filename = "Resources://Pipeline/Sprite.shader"